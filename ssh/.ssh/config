Host res-hpc-lo04.researchlumc.nl
	Compression yes
	ForwardX11 yes
	ServerAliveInterval 60
	ServerAliveCountMax 3
	Hostname res-hpc-lo04.researchlumc.nl
	User esrasche
	ProxyCommand ssh -q -X -C esrasche@res-ssh-alg01.researchlumc.nl nc %h %p

